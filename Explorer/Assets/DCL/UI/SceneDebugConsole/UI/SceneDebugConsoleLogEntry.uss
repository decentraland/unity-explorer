.log-entry {
    height: auto;
    align-items: flex-start;
    justify-content: center;
    padding-left: 10px;
    background-color: rgb(170, 89, 57);
    border-color: rgb(49, 26, 17);
    border-width: 2px;
    border-radius: 15px;
}

.log-entry > .unity-label {
    flex-grow: 1;
    flex-wrap: wrap;
    white-space: normal;
}
