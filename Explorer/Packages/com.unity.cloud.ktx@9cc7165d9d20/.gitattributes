# Denote all files that are truly binary and should not be modified.
*.so binary filter=lfs diff=lfs merge=lfs
*.a binary filter=lfs diff=lfs merge=lfs
*.bundle binary filter=lfs diff=lfs merge=lfs
*.dll binary filter=lfs diff=lfs merge=lfs
*.bc filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.bytes filter=lfs diff=lfs merge=lfs -text
Runtime/Plugins/x86_64/ktx_unity.bundle/Contents/MacOS/ktx_unity filter=lfs diff=lfs merge=lfs -text
